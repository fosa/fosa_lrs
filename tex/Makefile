.PHONY: all light clean

all: build clean

build:
	pdflatex --shell-escape fosalrs_main.tex
	pdflatex --shell-escape fosalrs_main.tex
	pdflatex --shell-escape fosalrs_main.tex

clean:
	rm -f *.aux
	rm -f *.bbl
	rm -f *.blg
	rm -f *.log
	rm -f *.out
	rm -f *.toc
	rm -f *.lot
	rm -f *.lof
	rm -f w18-test-*.tex
